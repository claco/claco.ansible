# {{ ansible_managed }}

if [ "$(ls -A {{ bash_profile_directory }})" ]; then
  for bash_profile in {{ bash_profile_directory }}/*; do
    [ -f "$bash_profile" ] || continue
    source ${bash_profile}
  done
fi

if [ "$(ls -A {{ bash_completion_directory }})" ]; then
  for bash_completion in {{ bash_completion_directory}}/*; do
    [ -f "$bash_completion" ] || continue
    source ${bash_completion}
  done
fi
